    "@unocss/reset": "^0.47.5",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/test-utils": "^2.2.6",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^12.0.1",
    "eslint": "^8.29.0",
